norm_U_L1
{
    type            norm;
    libs            ( fieldFunctionObjects );
    field           U;
    norm            L1;
    result          norm_U_L1;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  runTime;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
}

