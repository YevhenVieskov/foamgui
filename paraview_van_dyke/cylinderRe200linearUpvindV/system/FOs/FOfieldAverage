// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
fieldAverage
{
    type            fieldAverage;
    libs            ( fieldFunctionObjects );
    enabled         true;
    timeStart       0;
    timeEnd         400;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
    fields          ( U { mean on ; prime2Mean on ; base time ; } p { mean on ; prime2Mean on ; base time ; } );
    startTime       0;
}


