pressure1
{
    type            pressure;
    libs            ( fieldFunctionObjects );
    mode            static;
    U               U;
    rho             rhoInf;
    rhoInf          100000;
    pRef            0;
    result          staticP;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  runTime;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
}

