readFields1
{
    // Mandatory entries
    type            readFields;
    libs            (fieldFunctionObjects);
    fields          (U p);

    // Optional entries
    readOnStart     true;

    // Optional (inherited) entries
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         500;
    executeControl  runTime;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.01;
}


