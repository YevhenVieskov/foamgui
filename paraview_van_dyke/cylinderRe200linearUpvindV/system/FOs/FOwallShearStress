// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
wallShearStress1
{
    type            wallShearStress;
    libs            ( fieldFunctionObjects );
    patches         ( wall );
    writeFields     yes;
    writePrecision  10;
    writeToFile     true;
    useUserTime     true;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
}

magWallShearStress1
{
    type            mag;
    field           wallShearStress;
    libs            ( fieldFunctionObjects );
    patches         ( wall );
    writeFields     yes;
    writePrecision  10;
    writeToFile     true;
    useUserTime     true;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
}


