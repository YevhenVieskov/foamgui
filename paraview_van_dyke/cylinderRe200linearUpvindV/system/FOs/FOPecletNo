PecletNo1
{
    type            PecletNo;
    libs            ( fieldFunctionObjects );
    rho             rho;
    field           phi;
    result          PecletNoField;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  timeStep;
    executeInterval 1;
    writeToFile     true;
    useUserTime     true;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
}


