// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
momentum1
{
    type            momentum;
    libs            ( fieldFunctionObjects );
    regionType      all;
    writeMomentum   yes;
    writePosition   yes;
    writeVelocity   yes;
    p               p;
    U               U;
    rho             rho;
    rhoRef          1;
    cylindrical     false;
    writePrecision  10;
    writeToFile     true;
    useUserTime     true;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         400;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    adjustableRunTime;
    writeInterval   0.5;
    useNamePrefix   true;
}


